<div class="route route-jobs-state">
  {{jobs-table jobs=model selectedJob=selectedJob order=order orderAction="updateOrder"}}

  <div class="panel-right">
    {{#if selectedJob}}
      {{job-detail job=selectedJob action="goToJob" removeAction="removeJob"}}
    {{else}}
    {{/if}}
  </div>
  {{jobs-paging page=page}}
</div>
